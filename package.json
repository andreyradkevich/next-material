{
  "name": "next-material",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:write": "eslint src/ --fix --cache",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "pretty-quick --staged",
      "npm run lint:write",
      "git add ."
    ],
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "@material-ui/core": "4.12.2",
    "@material-ui/styles": "4.11.4",
    "axios": "0.21.1",
    "clsx": "1.1.1",
    "next": "11.0.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-string-replace": "0.4.4"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "babel-plugin-styled-components": "1.13.2",
    "eslint": "7.31.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.1",
    "lint-staged": "11.1.0",
    "prettier": "2.3.2",
    "pretty-quick": "3.1.1"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
